{
  "name": "vue-project",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development MODE=dev node server",
    "build": "run-p build:*",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server",
    "server": "cross-env NODE_ENV=production MODE=prod node server",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-proxy-middleware": "^2.0.6",
    "pinia": "^2.1.3",
    "serve-static": "^1.15.0",
    "vue": "^3.3.4",
    "vue-i18n": "^9.4.1",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.35.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/compression": "^1.7.3",
    "@types/express": "^4.17.17",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.17",
    "@unocss/preset-attributify": "^0.56.4",
    "@unocss/preset-uno": "^0.56.4",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "sass": "^1.68.0",
    "typescript": "~5.0.4",
    "unocss": "^0.56.4",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.5.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  },
  "engines": {
    "node": ">=16"
  }
}
